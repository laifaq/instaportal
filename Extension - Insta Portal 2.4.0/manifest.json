{
    "manifest_version": 3,
    "name": "Insta Portal Extention",
    "version": "2.4.0",
    "description": "Unlock Premium Bersama",
    "homepage_url": "https://premiumportal.id/",
    "action": {
      "default_popup": "./popup.html"
    },
    "permissions": [
      "cookies",
      "storage",
      "activeTab",
      "tabs",
      "scripting",
      "webRequest",
      "browsingData",
      "declarativeNetRequest",
      "declarativeNetRequestWithHostAccess",
      "alarms",
      "management",
      "downloads"
    ],
    "host_permissions": ["*://*/*"],
    "background": {
      "service_worker": "./js/background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["image/logo-up.png"],
        "matches": ["<all_urls>"]
      }
    ],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "windows": "Alt+Shift+G",
          "mac": "Alt+Shift+G",
          "chromeos": "Alt+Shift+G",
          "linux": "Alt+Shift+G"
        }
      }
    },
    "icons": {
      "16": "./image/icon16.png",
      "48": "./image/icon48.png",
      "128": "./image/icon128.png"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self' http://premiumportal.id http://cdn.premiumportal.id ;"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*", "*://chatgpt.com/*"],
        "js": ["./js/content.js"],
        "run_at": "document_start"
      }
    ]
  }
  